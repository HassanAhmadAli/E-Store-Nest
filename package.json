{
  "name": "e-store",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "reset:dev": "pnpm install --prefer-offline && pnpm run db:reset",
    "db:reset": "prisma migrate reset --force && prisma migrate dev --name e-store && pnpm run db:generate && prisma db seed",
    "format": "prettier --write \"{src,test,prisma}/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development NEST_DEBUG=true nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,test,prisma}/**/*.ts\" --fix",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:cov": "bun test --coverage",
    "test:debug": "bun test --inspect-brk -timeout 0",
    "test:e2e": "bun test **/*e2e-spec.ts",
    "test:e2e:win": "powershell -c \"$x = ls **/*e2e-spec.ts ; bun test $x\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name e-store",
    "db:studio": "prisma studio",
    "debug:circular-dependency": "pnpm run build && madge dist/src/main.js --circular"
  },
  "dependencies": {
