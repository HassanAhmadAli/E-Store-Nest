{
  "name": "e-store",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "cross-env NEST_DEBUG=true nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:cov": "bun test --coverage",
    "test:debug": "bun test --inspect-brk -timeout 0",
    "test:e2e": "bun test **/*e2e-spec.ts",
    "test:e2e:win": "powershell -c \"$x = ls **/*e2e-spec.ts ; bun test $x\"",
    "db:migrate": "prisma migrate dev --name e-store",
    "db:studio": "prisma studio",
    "debug:circular-dependency": "npm run build && madge dist/main.js --circular"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/middie": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.17.1",
    "@types/morgan": "^1.9.10",
    "argon2": "^0.44.0",
    "cache-manager": "^7.2.4",
    "cross-env": "^10.1.0",
    "fastify": "^5.6.1",
    "google-auth-library": "^10.4.1",
    "googleapis": "^164.0.0",
    "ioredis": "^5.8.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nestjs-zod": "^5.0.1",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "piscina": "^5.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "madge": "^8.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.17.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}